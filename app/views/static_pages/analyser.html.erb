<div class="row bg-title">
  <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
    <h4 class="page-title">Analyser</h4>
  </div>
  <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
    <ol class="breadcrumb">
      <li><a href="#">Main menu</a></li>
      <li class="active">Analyser</li>
    </ol>
  </div>
</div>
<div id="headerTextRow" class="row">
  <div class="col-lg-12">
    <div class="white-box">
      <div>
        <h3 class="box-title">Paste the header below</h3>
        <div class="form-group">
          <textarea class="form-control" id="headerInput" rows="15" placeholder=""></textarea>
        </div>
        <div class="checkbox checkbox-success" id="saveBoxDiv">
          <input id="saveBox" type="checkbox">
          <label for="saveBox"> Save this </label>
          <br>
        </div>
        <div class="form-group" id="additionalInfo">
          <hr>
          <label>Name</label>
          <input type="text" class="form-control" id="headerNameInput" placeholder="Name your header">
          <br>
          <label>Description</label>
          <textarea class="form-control" id="headerDescriptionInput" rows="3" placeholder="Optional"></textarea>
        </div>
        <div id="buttonGroup">
          <hr>
          <button id="analyseButton" class="btn btn-primary"><i class="fa fa-bolt"></i> Analyse</button>
        </div>
      </div>
    </div>
  </div>
</div>
